<Application x:Class="XfPrint.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfAppMain"
             DispatcherUnhandledException="Application_DispatcherUnhandledException"
              >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/ResourceLib;component/Language/cn.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ResourceLib;component/Style/Style1/Default.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
